{
    "version": "0.1.1",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/esm/index.mjs",
    "types": "dist/types/index.d.mts",
    "typings": "dist/types/index.d.mts",
    "files": [
        "dist/**/*"
    ],
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "build": "ts-build build",
        "lint": "ts-build lint",
        "size": "size-limit",
        "test": "jest",
        "prettier": "prettier --write src/**/*"
    },
    "husky": {
        "hooks": {
            "pre-commit": "eslint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "exports": {
        ".": {
            "import": {
                "types": "./dist/esm/index.d.ts",
                "typings": "./dist/esm/index.d.ts",
                "default": "./dist/esm/index.mjs"
            },
            "require": {
                "types": "./dist/cjs/index.d.ts",
                "typings": "./dist/cjs/index.d.ts",
                "default": "./dist/cjs/index.cjs"
            }
        }
    },
    "name": "@azlabsjs/test",
    "author": "",
    "size-limit": [
        {
            "path": "dist/cjs/index.cjs",
            "limit": "20 KB"
        },
        {
            "path": "dist/esm/index.mjs",
            "limit": "20 KB"
        }
    ],
    "devDependencies": {
        "@azlabsjs/ts-build": "^0.5.11",
        "@babel/runtime": "^7.27.6",
        "@eslint/js": "^9.30.0",
        "@size-limit/preset-small-lib": "^11.2.0",
        "@types/jest": "^30.0.0",
        "@typescript-eslint/eslint-plugin": "^8.35.0",
        "@typescript-eslint/parser": "^8.35.0",
        "eslint": "^9.30.0",
        "husky": "^9.1.7",
        "jest": "^30.0.3",
        "jest-watch-typeahead": "^3.0.1",
        "prettier": "^3.6.2",
        "size-limit": "^11.2.0",
        "ts-jest": "^29.4.0",
        "tslib": "^2.8.1",
        "typescript": "^5.8.3"
    }
}
